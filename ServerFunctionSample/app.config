<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<appSettings>
		<!-- Azure configuration -->
		<!-- The Azure AD instance to use in authentication. This URL will be concatenated with the Tenant to form the authentication authority URL. DO NOT TERMINATE WITH A SLASH. -->
		<add key="ida:AADInstance" value="https://login.microsoftonline.com" />
		<!-- The name of the tenant to authenticate to. -->
		<add key="ida:Tenant" value="" />
		<!-- Id of the client application, as retrieved from the Azure AD registered apps. -->
		<add key="ida:ClientId" value="" />
		<!-- The key for the client application, as obtained when adding a new key to the app. NOTE THAT THE KEY CANNOT BE RETRIEVED AT A LATER TIME, ONLY ON KEY CREATION. -->
		<add key="ida:AppKey" value="" />
		<!-- The URL to the resource to get an access token to. -->
		<add key="ida:ResourceUri" value="" />
		<!-- Loop configuration -->
		<!-- URL to get an authenticated session from a JWT Bearer token. -->
		<add key="loop:Authorisation" value="" />
		<!-- Loop server function URL. -->
		<add key="loop:ServerFunction" value="" />
	</appSettings>
</configuration>